{
  "schemaVersion": 1,
  "id": "hexportation",
  "version": "${version}",
  "name": "Hexportation",
  "description": "Hexcasting addon for moving items/fluids/energy around",
  "authors": [
    "kineticcat"
  ],
  "contact": {
    "homepage": "https://kineticcat.github.io/hexportation",
    "sources": "https://github.com/kineticcat/hexportation"
  },
  "license": "MIT",
  "icon": "icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "dev.kineticcat.hexportation.fabric.HexportationFabric"
    ],
    "client": [
      "dev.kineticcat.hexportation.fabric.HexportationClientFabric"
    ]
  },
  "mixins": [
    "hexportation-common.mixins.json",
    "hexportation-fabric.mixins.json"
  ],
  "depends": {
    "minecraft": ">=${minecraftVersion}",
    "fabricloader": ">=${fabricLoaderVersion}",
    "fabric-api": "*",
    "fabric-language-kotlin": ">=${fabricKotlinVersion}",
    "architectury": ">=${architecturyVersion}",
    "hexcasting": ">=${hexcastingVersion}"
  }
}
